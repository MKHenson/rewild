{
  "name": "re-wild",
  "version": "0.0.2",
  "description": "Game about exploring earth's history",
  "main": "index.js",
  "scripts": {
    "build-test": "asc src/as/tests.ts --target test",
    "mocha": "node --experimental-wasi-unstable-preview1 --experimental-wasm-simd node_modules/mocha/bin/_mocha './tests/init.spec.ts' 'tests/**/*.spec.ts' --reporter spec",
    "test": "npm run build-test && npm run mocha",
    "benchmark": "cross-env TS_NODE_PROJECT=\"./tests/tsconfig.json\" node --experimental-wasi-unstable-preview1 --experimental-wasm-simd --require ts-node/register ./tests/benchmarks.ts",
    "clean": "rimraf public/untouched.wasm && rimraf public/untouched.wasm.map && rimraf public/untouched.wat",
    "asbuild": "asc src/as/index.ts --target release",
    "build:webpack": "webpack --mode development",
    "build": "npm run clean && npm run asbuild && npm run build:webpack",
    "watch-webpack": "webpack --mode development --watch --env env=local",
    "watch-as": "nodemon -e js,ts,tsx --watch src/as -x \"npm run asbuild\"",
    "start": "npm run build && npm-run-all --parallel server watch-webpack watch-as",
    "server": "live-server --port=9001 --watch=public,build --open=/ ./public"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/MathewKHenson/rewild-web.git"
  },
  "author": "Mathew Henson",
  "license": "ISC",
  "homepage": "https://bitbucket.org/MathewKHenson/rewild-web#readme",
  "dependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.14.7",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-typescript": "^7.14.5",
    "@types/node": "^16.11.6",
    "@urql/core": "3.0.3",
    "babel-loader": "^8.1.0",
    "babel-preset-solid": "1.3.13",
    "expect.js": "^0.3.1",
    "firebase": "^9.10.0",
    "fork-ts-checker-webpack-plugin": "7.2.1",
    "graphql": "^16.6.0",
    "solid-js": "1.3.13",
    "solid-styled-components": "0.27.6",
    "source-map-support": "^0.5.19"
  },
  "devDependencies": {
    "@types/expect.js": "0.3.29",
    "@types/mocha": "8.2.2",
    "@webgpu/types": "0.1.21",
    "assemblyscript": "0.20.6",
    "copy-webpack-plugin": "^9.0.1",
    "cross-env": "7.0.3",
    "file-loader": "^6.2.0",
    "graphql-tag": "2.12.6",
    "live-server": "1.2.2",
    "mocha": "10.0.0",
    "nodemon": "2.0.19",
    "npm-run-all": "4.1.5",
    "prettier": "2.3.2",
    "raw-loader": "^4.0.2",
    "rimraf": "3.0.2",
    "ts-loader": "9.2.3",
    "ts-node": "10.7.0",
    "typescript": "4.3.2",
    "webpack": "5.70.0",
    "webpack-cli": "4.7.0"
  }
}
